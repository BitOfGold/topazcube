{
  "name": "topazcube",
  "version": "0.1.34",
  "description": "TopazCube is a real-time collaborative document editing, multiplayer game library, and WebGPU renderer.",
  "author": "László Matuska @BitOfGold",
  "license": "Apache-2.0",
  "homepage": "https://topazcube.com",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BitOfGold/topazcube.git"
  },
  "bugs": {
    "url": "https://github.com/BitOfGold/topazcube/issues"
  },
  "files": [
    "src",
    "dist/*",
    "LICENSE",
    "README.md"
  ],
  "keywords": [
    "multiplayer",
    "multiplayer game",
    "multiplayer server",
    "multiplayer client",
    "multiplayer document",
    "collaborative document",
    "multiplayer editor",
    "collaborative editor",
    "multiplayer game",
    "collaborative game",
    "webgpu",
    "renderer"
  ],
  "sideEffects": false,
  "exports": {
    "./client": {
      "import": "./dist/client.js",
      "require": "./dist/client.cjs"
    },
    "./server": {
      "import": "./dist/server.js",
      "require": "./dist/server.cjs"
    },
    "./terminal": {
      "import": "./dist/terminal.js",
      "require": "./dist/terminal.cjs"
    },
    "./renderer": {
      "import": "./dist/Renderer.js",
      "require": "./dist/Renderer.cjs"
    }
  },
  "type": "module",
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "clean": "rimraf dist"
  },
  "devDependencies": {
    "rimraf": "^6.0.1",
    "vite": "^6.0.0",
    "vite-plugin-glsl": "^1.5.5"
  },
  "dependencies": {
    "bufferutil": "^4.0.9",
    "fast-json-patch": "^3.1.1",
    "gl-matrix": "^3.4.4",
    "mongodb": "^6.18.0",
    "msgpackr": "^1.11.5",
    "patch-package": "^8.0.0",
    "util": "^0.12.5",
    "werift": "^0.22.2",
    "ws": "^8.18.3",
    "@loaders.gl/core": "^4.3.4",
    "@loaders.gl/gltf": "^4.3.4",
    "lil-gui": "^0.21.0",
    "webgpu-utils": "^2.0.2",
    "wgsl_reflect": "^1.2.3"
  }
}
